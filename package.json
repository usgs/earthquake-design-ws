{
  "name": "earthquake-design-ws",
  "version": "0.1.0",
  "description": "Seismic design parameters webservice",
  "repository": {
    "type": "git",
    "url": "https://github.com/usgs/earthquake-design-ws.git"
  },
  "keywords": [
    "usgs"
  ],
  "scripts": {
    "coverage": "istanbul cover _mocha -- -u exports \"test/**/*Test.js\" -R spec",
    "dev": "npm-run-all --parallel watch start",
    "install": "node src/lib/pre-install",
    "lint": "eslint \"**/*.js\"",
    "start": "nodemon --watch src src/server.js",
    "test": "mocha \"test/**/*Test.js\" --reporter spec",
    "uninstall": "node src/lib/uninstall",
    "watch": "watch \"npm run lint && npm run test\" src test"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "license": "CC0-1.0",
  "dependencies": {
    "express": "^4.14.0",
    "extend": "^3.0.0",
    "inquirer": "^1.1.2",
    "nodemon": "^1.9.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.2",
    "npm-run-all": "^3.0.0",
    "pg-promise": "^5.3.2",
    "pre-commit": "^1.1.3",
    "sinon": "^1.17.4",
    "watch": "^0.19.1"
  },
  "engines": {
    "node": ">=4.5.0"
  }
}
